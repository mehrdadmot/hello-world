
pipeline {
  
  agent any
  
  stages {
    
    stage("checkout") {
    
      steps {
        echo 'checkout the code...!'  
      }
      
    }
    
    
    stage("build") {
    
      steps {
        echo 'build the code...!'  
      }
      
    }
    
    
    stage("deploy") {
    
      steps {
        echo 'deploy the code...!'  
      }
      
    }
    
  }
  
}
